<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet author="Neshin Ivan" id="20180929-01" objectQuotingStrategy="QUOTE_ALL_OBJECTS">
        <validCheckSum>ANY</validCheckSum>
        <createTable tableName="DownloadFileInfo" schemaName="Downloader">
            <column name="DownloadFileInfoId" type="BIGINT" remarks="Идентификатор">
                <constraints nullable="false"/>
            </column>
            <column name="VersionId" type="BIGINT" remarks="Идентификатор версии">
                <constraints nullable="false"/>
            </column>
            <column name="TextVersion" type="VARCHAR(1000)" remarks="Описание версии файла в текстовом виде"/>
            <column name="FiasCompleteDbfUrl" type="VARCHAR(2083)" remarks="URL полной версии ФИАС в формате DBF"/>
            <column name="FiasCompleteXmlUrl" type="VARCHAR(2083)" remarks="URL полной версии ФИАС в формате XML"/>
            <column name="FiasDeltaDbfUrl" type="VARCHAR(2083)" remarks="URL дельта версии ФИАС в формате DBF"/>
            <column name="FiasDeltaXmlUrl" type="VARCHAR(2083)" remarks="URL дельта версии ФИАС в формате XML"/>
            <column name="IsSaveLocalFile" type="BOOLEAN" remarks="Признак файл загружен локально"/>
        </createTable>
        <addPrimaryKey columnNames="DownloadFileInfoId" constraintName="user_pkey" tableName="DownloadFileInfo"/>
    </changeSet>

</databaseChangeLog>